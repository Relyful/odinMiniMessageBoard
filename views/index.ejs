<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="styles.css" />
  </head>
  <body>
    <h1>Relyfuls's Message Board</h1>

    <div class="messages">
      <% messages.forEach((message) => { %>
        <%- include('message', {user: message.username, text: message.text, added: message.added}); %>
      <% }); %>
    </div>
    <br>
    <a href="/new"><button>New Message</button></a>
  </body>
</html>